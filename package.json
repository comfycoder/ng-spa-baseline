{
  "name": "my-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "start:hmr": "ng serve --configuration hmr --open",
    "start:prod": "ng serve --prod --open",
    "hmr": "ng serve --hmr -e=hmr --open",
    "build": "ng build",
    "build:prod": "ng build --prod --base-href=./",
    "build:prod:folder": "ng build --prod --base-href=/my-app/",
    "git": "ng build --prod --output-path docs --base-href=./",
    "test": "ng test",
    "test:ci": "ng test --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "lint": "ng lint --type-check --format stylish my-app my-app-e2e",
    "lint:fix": "ng lint --type-check --fix --format stylish my-app my-app-e2e",
    "lint:ci": "ng lint --type-check my-app my-app-e2e",
    "e2e": "ng e2e",
    "e2e:ci": "ng e2e --protractor-config=./e2e/protractor-ci.conf.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular/cli": "~6.1.5",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@angularclass/hmr": "^2.1.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
